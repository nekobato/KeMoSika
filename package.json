    "electron-builder": "^24.6.4",
    "electron-icon-builder": "^2.0.1",
    "fs-extra": "^11.2.0",
    "nanoid": "^5.0.4",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5",
